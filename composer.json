{
  "name": "solvers/dsql",
  "type": "project",
  "description": "Solvers BD data collector web application",
  "autoload": {
    "psr-4": {
      "Solvers\\Dsql\\": "app/",
      "Solvers\\Ratan\\": "ratan/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "authors": [
