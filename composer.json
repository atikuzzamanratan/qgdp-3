    "nette/database": "^3.1",
    "guzzlehttp/guzzle": "^7.7",
    "phpauth/phpauth": "^1.5",
    "nette/utils": "^3.2",
    "ezyang/htmlpurifier": "^4.16",
    "shieldon/shieldon": "^2.1",
    "nette/di": "^3.1",
    "nette/neon": "^3.3",
    "nette/security": "^3.1"
